#, python-format
msgid " in %d seconds."
msgstr " w %d sekundy."

msgid "Cancel"
msgstr "Anuluj"

msgid "FILE NOT EXIST : /proc/stb/fp/remote_code"
msgstr "PLIK NIE ISTNIEJE : /proc/stb/fp/remote_code"

msgid "Keep"
msgstr "Zachowaj"

msgid "Please change your remote mode"
msgstr "Zmień swój tryb pilota"

msgid "Press and hold '2' & '7' until red LED is solid, then press 'Help', then press '000"
