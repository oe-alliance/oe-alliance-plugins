	<servicehacks>
<![CDATA[
blacklist = ['M Pack Tier 1','More TV Pack Tier 2','M+ Pack Tier 3','L Pack Tier 4','XL Pack Tier 5'] # channel names have quotes. Channel numbers do not. Example: ['ITV HD', 250, 500]
bt_sports_xtra = ["BT Sport Extra 1","BT Sport Extra 2","BT Sport Extra 3","BT Sport Extra 4","BT Sport Extra 5","BT Sport Extra 6"]

if service["service_type"] == 1 and service["service_group_id"] in(17,21):
	service["service_type"] = 25

for number in service["numbers"]:
	if number in blacklist:
		skip = True
		break
	elif number >= 50 and number <= 64:
		service["numbers"] = [number + 951]
		break
	elif number == 43:
		service["numbers"] = [number + 957]
		break
	elif number < 100 and service["service_name"] in bt_sports_xtra:
		service["numbers"] = [1020 + bt_sports_xtra.index(service["service_name"])]
		break
		
if service["service_name"] in blacklist:
	skip = True
		
if service["service_name"].startswith("DL_") or service["service_name"].startswith("Hidden"):
	skip = True
	
if service["service_name"].startswith("SptsETV"):
	service["free_ca"] = 1
	
]]>
